Please return  response => response\n  */\n\n  /**\n   * Determine the request status by custom code\n   * Here is just an example\n   * You can also judge the status by HTTP Status Code\n   */\n  response => {\n    const res = response.data\n    // if the custom code is not 20000, it is judged as an error.\n    if (res.code == -1) {\n      Message({\n        message: res.msg || 'Error',\n        type: 'error',\n        duration: 5 * 1000\n      })\n      return Promise.reject(new Error(res.message || 'Error'))\n    } else {\n      return res\n    }\n  },\n  error => {\n    console.log('err' + error) // for debug\n    Message({\n      message: error.message,\n      type: 'error',\n      duration: 5 * 1000\n    })\n    return Promise.reject(error)\n  }\n)\n\nexport default service\n"],"mappings":";;AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,UAAT,EAAqBC,OAArB,QAAoC,YAApC;AAEA,OAAO,IAAMC,OAAO,GAAG,uBAAhB,C,CACP;;AACA,IAAMC,OAAO,GAAGJ,KAAK,CAACK,MAAN,CAAa;EAC3BF,OAAO,EAAPA,OAD2B;EAClB;EACTG,eAAe,EAAE,IAFU;EAEJ;EACvBC,OAAO,EAAE,KAHkB,CAGZ;;AAHY,CAAb,CAAhB,C,CAMA;;AACAH,OAAO,CAACI,YAAR,CAAqBC,OAArB,CAA6BC,GAA7B,CACE,UAAAC,MAAM,EAAI;EACR;EACAA,MAAM,CAACC,OAAP,CAAe,cAAf,IAAiC,kBAAjC;EACA,OAAOD,MAAP;AACD,CALH,EAME,UAAAE,KAAK,EAAI;EACP;EACAC,OAAO,CAACC,GAAR,CAAYF,KAAZ,EAFO,CAEY;;EACnB,OAAOG,OAAO,CAACC,MAAR,CAAeJ,KAAf,CAAP;AACD,CAVH,E,CAaA;;AACAT,OAAO,CAACI,YAAR,CAAqBU,QAArB,CAA8BR,GAA9B;AACE;AACF;AACA;AACA;;AAEE;AACF;AACA;AACA;AACA;AACE,UAAAQ,QAAQ,EAAI;EACV,IAAMC,GAAG,GAAGD,QAAQ,CAACE,IAArB,CADU,CAEV;;EACA,IAAID,GAAG,CAACE,IAAJ,IAAY,CAAC,CAAjB,EAAoB;IAClBnB,OAAO,CAAC;MACNoB,OAAO,EAAEH,GAAG,CAACI,GAAJ,IAAW,OADd;MAENC,IAAI,EAAE,OAFA;MAGNC,QAAQ,EAAE,IAAI;IAHR,CAAD,CAAP;IAKA,OAAOT,OAAO,CAACC,MAAR,CAAe,IAAIS,KAAJ,CAAUP,GAAG,CAACG,OAAJ,IAAe,OAAzB,CAAf,CAAP;EACD,CAPD,MAOO;IACL,OAAOH,GAAP;EACD;AACF,CAxBH,EAyBE,UAAAN,KAAK,EAAI;EACPC,OAAO,CAACC,GAAR,CAAY,QAAQF,KAApB,EADO,CACoB;;EAC3BX,OAAO,CAAC;IACNoB,OAAO,EAAET,KAAK,CAACS,OADT;IAENE,IAAI,EAAE,OAFA;IAGNC,QAAQ,EAAE,IAAI;EAHR,CAAD,CAAP;EAKA,